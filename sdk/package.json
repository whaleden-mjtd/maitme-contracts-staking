{
  "name": "@maitme/staking-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for MAIT Progressive Staking Contract",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "keywords": [
    "ethereum",
    "staking",
    "defi",
    "mait",
    "web3"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/whaleden-mjtd/maitme-contracts-staking.git"
  }
